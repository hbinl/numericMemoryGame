<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hb.neuropsych_5_numeric" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" >
        <activity
            android:name="com.hb.neuropsych_5_numeric.NumericStart"
            android:noHistory="true"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.hb.neuropsych_5_numeric.NumericMainActivity"
            android:noHistory="true"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_numeric_main" >
        </activity>
        <activity
            android:name="com.hb.neuropsych_5_numeric.NumericMainInput"
            android:noHistory="true"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_numeric_main_input" >
        </activity>
        <activity
            android:name="com.hb.neuropsych_5_numeric.NumericEndReport"
            android:noHistory="true"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_numeric_end_report"
            android:parentActivityName="com.hb.neuropsych_5_numeric.NumericStart" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.hb.neuropsych_5_numeric.NumericStart" />
        </activity>
    </application>

</manifest>
